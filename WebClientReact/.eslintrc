{
    "extends": "rackt",
    "globals": {
        "__DEV__": true
    },
    "rules": {
        "react/jsx-uses-react": 1,
        "react/jsx-no-undef": 2,
        "react/wrap-multilines": 2,
        "indent": [2, 4],
        "react/jsx-indent": [2, 4]
    },
    "plugins": [
        "react"
    ]
}
